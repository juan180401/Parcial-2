<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/registrocars"
    tools:context=".RegistroDeCarrosPorBusqueda">

    <EditText
        android:id="@+id/color5"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="260dp"
        android:ems="10"
        android:hint="Color:"
        android:inputType="textPersonName"
        android:textColor="#FFFB00"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/codigo5"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="124dp"
        android:ems="10"
        android:hint="Codigo:"
        android:inputType="number"
        android:textColor="#FFFB00"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/usulog6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="68dp"
        android:text="TextView"
        android:textColor="#FF0101"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/modelo5"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="328dp"
        android:ems="10"
        android:hint="Modelo:"
        android:inputType="textPersonName"
        android:textColor="#FFFB00"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Precio5"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="396dp"
        android:ems="10"
        android:hint="Precio:"
        android:inputType="textPersonName"
        android:textColor="#FFFB00"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="28dp"
        android:text="REGISTRO DE AUTOS"
        android:textColor="#FBFBFB"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button25"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="576dp"
        android:backgroundTint="#F60000"
        android:onClick="volv"
        android:text="VOLVER"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button26"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="488dp"
        android:backgroundTint="#005EFF"
        android:onClick="verificarregis"
        android:text="VERIFICAR REGISTROS"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/marca5"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="192dp"
        android:ems="10"
        android:hint="Marca:"
        android:inputType="textPersonName"
        android:textColor="#FFFB00"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>