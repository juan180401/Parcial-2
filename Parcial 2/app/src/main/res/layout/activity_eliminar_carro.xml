<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/carro"
    tools:context=".EliminarCarro">

    <Button
        android:id="@+id/button11"
        android:layout_width="150dp"
        android:layout_height="47dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="416dp"
        android:backgroundTint="#454545"
        android:onClick="verificar1"
        android:text="VERIFICAR"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/color3"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="260dp"
        android:ems="10"
        android:hint="Color:"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/codigo3"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="124dp"
        android:ems="10"
        android:hint="Codigo:"
        android:inputType="number"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/marca3"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="192dp"
        android:ems="10"
        android:hint="Marca:"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button12"
        android:layout_width="150dp"
        android:layout_height="47dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="484dp"
        android:backgroundTint="#FD0000"
        android:onClick="eliminar"
        android:text="ELIMINAR"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button13"
        android:layout_width="150dp"
        android:layout_height="47dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="548dp"
        android:backgroundTint="#454545"
        android:onClick="volv"
        android:text="VOLVER"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/usulog4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="68dp"
        android:text="TextView"
        android:textColor="#FF0101"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginTop="28dp"
        android:text="ELIMINAR AUTO"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/modelo3"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="328dp"
        android:ems="10"
        android:hint="Modelo:"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFB00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>